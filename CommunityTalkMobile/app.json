{
  "expo": {
    "name": "Campustry",
    "slug": "campustry-debugdragons",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./public/logo.png",
    "scheme": "communitytalkmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.communitytalk.mobile",
      "buildNumber": "1",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": true,
        "NSCameraUsageDescription": "This app uses the camera for verifying your profile and taking photos for messages.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record voice messages in chats.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photos to let you share images in chats and update your profile.",
        "NSPhotoLibraryAddUsageDescription": "This app saves photos to your library."
      }
    },
    "android": {
      "package": "com.communitytalk.mobile",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "backgroundColor": "#FAF9F7",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-notifications",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "API_URL_DEV": "http://localhost:3000",
      "API_URL_PROD": "https://api.campustry.com",
      "router": {},
      "eas": {
        "projectId": "f68cb752-501d-4545-a5fd-5a5cbfb85f5d"
      }
    }
  }
}